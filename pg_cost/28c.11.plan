                                                              Hash Cond: (mk.keyword_id = k.id)
                                                              ->  Nested Loop
                                                                    ->  Hash Join
                                                                          Hash Cond: (t.id = cc.movie_id)
                                                                          ->  Seq Scan on title t
                                                                                Filter: (production_year > 2005)
                                                                          ->  Hash
                                                                                ->  Hash Join
                                                                                      Hash Cond: (cc.subject_id = cct1.id)
                                                                                      ->  Seq Scan on complete_cast cc
                                                                                      ->  Hash
                                                                                            ->  Seq Scan on comp_cast_type cct1
                                                                                                  Filter: ((kind)::text = 'cast'::text)
