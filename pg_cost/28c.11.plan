                          ->  Nested Loop
                                Join Filter: (t.id = mc.movie_id)
                                ->  Nested Loop
                                      Join Filter: (t.id = mi_idx.movie_id)
                                      ->  Hash Join
                                            Hash Cond: (cc.status_id = cct2.id)
